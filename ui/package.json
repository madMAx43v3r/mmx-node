{
  "name": "mmx-node-web-gui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "bunx --bun eslint  ./ --fix",
    "format": "bunx --bun prettier --write \"**/*.{js,vue,scss,html,md,json}\" --list-different --ignore-path .gitignore --ignore-path .prettierignore",
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "dev:explorer": "bunx --bun vite --config vite.config.explorer.js",
    "build:explorer": "bunx --bun vite build --config vite.config.explorer.js",
    "preview:explorer": "bunx --bun vite preview --config vite.config.explorer.js",
    "dev:offline": "bunx --bun vite --config vite.config.offline.js",
    "build:offline": "bunx --bun vite build --config vite.config.offline.js",
    "preview:offline": "bunx --bun vite preview --config vite.config.offline.js",
    "build:all": "bun run build && bun run build:explorer && bun run build:offline",
    "build:android": "bun run build:explorer && bunx cap sync && bunx capacitor-assets generate --android && bunx cap build android"
  },
  "dependencies": {
    "@capacitor/android": "^7.1.0",
    "@capacitor/core": "^7.1.0",
    "@fontsource-variable/roboto-flex": "^5.2.5",
    "@fontsource-variable/roboto-mono": "^5.2.5",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@mdi/js": "^7.4.47",
    "@noble/hashes": "^1.7.1",
    "@noble/secp256k1": "^2.2.3",
    "@quasar/extras": "^1.16.17",
    "@scure/base": "^1.2.4",
    "@scure/bip39": "1.5.4",
    "@tanstack/vue-query": "^5.69.0",
    "@tanstack/vue-query-devtools": "^5.69.0",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0",
    "@vueuse/integrations": "^13.0.0",
    "animate.css": "^4.1.1",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "echarts": "^5.6.0",
    "fflate": "^0.8.2",
    "highlight.js": "^11.11.1",
    "json-bigint": "^1.0.0",
    "pinia": "^3.0.1",
    "qrcode": "^1.5.4",
    "quasar": "^2.18.1",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.1.0",
    "@eslint/compat": "^1.2.7",
    "@quasar/vite-plugin": "^1.9.0",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.0.9",
    "bun": "1.1.24",
    "bunx": "^0.1.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.2",
    "vite-plugin-generate-file": "^0.2.0",
    "vite-plugin-singlefile": "^2.2.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@9.15.5"
}
